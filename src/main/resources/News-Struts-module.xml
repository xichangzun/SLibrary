<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="news" namespace="/News" extends="struts-default">
        <action name="add" class="newsAction" method="add">
        </action>
        <action name="update" class="newsAction" method="update">
        </action>
        <action name="detail" class="newsAction" method="detail">
            <result name="success" type="redirect" >/admin_news_update.jsp</result>
        </action>
        <action name="query" class="newsAction" method="query">
            <result name="success" type="redirect">/admin_newsManage.jsp</result>
        </action>
        <action name="callback" class="newsAction" method="callback">
        </action>
        <action name="callres" class="newsAction" method="callres">
        </action>
        <action name="KnowMore" class="newsAction" method="detail">
            <result name="success" type="redirect">/news_detail.jsp</result>
        </action>
    </package>
</struts>